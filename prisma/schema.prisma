generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider          = "postgres"
  url               = env("DATABASE")
  shadowDatabaseUrl = env("SHADOW_DATABASE")

}

model Joke {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  clown     String
  content   String
}
